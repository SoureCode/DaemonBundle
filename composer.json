{
  "name": "sourecode/daemon-bundle",
  "type": "symfony-bundle",
  "license": "MIT",
  "minimum-stability": "dev",
  "autoload": {
    "psr-4": {
      "SoureCode\\Bundle\\Daemon\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "SoureCode\\Bundle\\Daemon\\Tests\\": "tests/"
    }
  },
  "authors": [
    {
      "name": "chapterjason",
      "email": "jason@sourecode.dev"
    }
  ],
  "scripts": {
    "test": [
      "simple-phpunit"
    ]
  },
  "require": {
    "php": ">=8.2",
    "ext-dom": "*",
    "ext-simplexml": "*",
    "ext-posix": "*",
    "symfony/http-kernel": "^6.3 || ^6.4",
    "symfony/console": "^6.3 || ^6.4",
    "symfony/process": "^6.3 || ^6.4",
    "symfony/config": "^6.3 || ^6.4",
    "symfony/dependency-injection": "^6.3 || ^6.4",
    "symfony/finder": "^6.3 || ^6.4"
  },
  "require-dev": {
    "nyholm/symfony-bundle-test": "^2.0",
    "phpunit/phpunit": "^9.5",
    "symfony/phpunit-bridge": "^6.3 || ^6.4"
  }
}
